{
  "name": "datasirpi_web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "node scripts/generate-sitemap.js",
    "build": "react-scripts build",
    "prerender": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "aos": "^2.3.4",
    "bootstrap": "^5.3.3",
    "css-loader": "^7.1.2",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.6",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.0.2",
    "react-scripts": "5.0.1",
    "react-snap": "^1.23.0",
    "react-transition-group": "^4.4.5",
    "sass": "^1.82.0",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "inlineCss": true,
    "source": "build",
    "destination": "build",
    "include": [
      "/",
      "/about",
      "/platform-engineering",
      "/application-engineering",
      "/salesforce-crm",
      "/cyber-security",
      "/ai-agents",
      "/case-studies",
      "/blogs",
      "/privacy",
      "/terms"
    ]
  }
}
